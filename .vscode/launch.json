{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Flutter: main.dart",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart"
        },
        {
            "name": "Python: Release Build",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/release_build.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "--config=my_secrets.txt"
            ]
        }
    ]
}